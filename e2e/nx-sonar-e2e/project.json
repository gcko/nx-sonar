{
  "name": "nx-sonar-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/nx-sonar-e2e/tests",
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "e2e/nx-sonar-e2e/jest.config.ts",
        "runInBand": true,
        "passWithNoTests": false
      },
      "dependsOn": ["nx-sonar:build"]
    },
    "sonar": {
      "executor": "@gcko/nx-sonar:scan",
      "options": {
        "name": "nx-sonar",
        "hostUrl": "https://sonarcloud.io",
        "organization": "gcko",
        "projectKey": "nx-sonar",
        "skipTargetDefaults": false,
        "branches": false,
        "qualityGate": true,
        "qualityGateTimeout": "300",
        "skipImplicitDeps": false
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["nx-sonar"]
}
